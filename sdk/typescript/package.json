{
  "name": "@youtube-subtitle-api/sdk",
  "version": "1.0.0",
  "description": "TypeScript SDK for YouTube Subtitle API - Extract subtitles with ease",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    },
    "./webhook": {
      "import": {
        "types": "./dist/webhook.d.ts",
        "default": "./dist/webhook.js"
      },
      "require": {
        "types": "./dist/webhook.d.cts",
        "default": "./dist/webhook.cjs"
      }
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:run": "vitest run",
    "lint": "eslint src",
    "lint:fix": "eslint src --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "youtube",
    "subtitle",
    "caption",
    "transcript",
    "api",
    "sdk",
    "typescript",
    "video",
    "youtube-api"
  ],
  "author": "ExpertBeacon",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/expertbeacon/youtube-subtitle-api"
  },
  "bugs": {
    "url": "https://github.com/expertbeacon/youtube-subtitle-api/issues"
  },
  "homepage": "https://expertbeacon.com/youtube-subtitle-api",
  "engines": {
    "node": ">=18.0.0"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/node": "^20.10.0",
    "eslint": "^8.55.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {},
  "publishConfig": {
    "access": "public"
  }
}
